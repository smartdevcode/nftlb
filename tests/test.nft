table netdev nftlb {
	map tcp-services-enp0s8 {
		type ipv4_addr . inet_service : verdict
		elements = { 192.168.56.2 . 80 : goto lb01 }
	}

	chain ingress-enp0s8 {
		type filter hook ingress device "enp0s8" priority 100; policy accept;
		ip daddr . tcp dport vmap @tcp-services-enp0s8
	}

	chain lb01 {
		ether saddr set 08:00:27:99:97:74 ether daddr set jhash ip saddr mod 10 map { 0-4 : 0a:00:27:00:00:00, 5-9 : 00:03:2d:13:0f:0c } fwd to ether daddr map { 0a:00:27:00:00:00 : "enp0s8", 00:03:2d:13:0f:0c : "enp0s3" }
	}
}
